ssb_zeta_system_initializer = {
	name = "Zeta"
	class = "sc_m"
    usage = misc_system_init

    usage_odds = {
        base = 2000000
    modifier = {
        factor = 0
        is_bottleneck_system = yes
        }
    }
    max_instances = 1

    init_effect = {
        generate_home_system_resources = yes
    }
	planet = {
		name = "Zeta"
		class = "pc_m_star"
		orbit_distance = 0
		orbit_angle = 1
		size = 44
		has_ring = no
	}

	planet = {
		name = "Molto"
		class = "pc_molten"
		orbit_distance = 75
		orbit_angle = { min = 90 max = 270 }
		size = 5
		has_ring = no
		tile_blockers = none
		modifiers = none
		init_effect = {
			prevent_anomaly = yes
		}
	}
	planet = {
		name = "Loirer X"
		class = "pc_molten"
		orbit_distance = 35
		orbit_angle = { min = 90 max = 270 }
		size = 15
		has_ring = yes
